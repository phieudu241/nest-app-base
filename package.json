{
  "name": "nest-typescript-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "nest start --watch",
    "debug": "nest start --debug --watch",
    "start": "nest start",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:seed": "ts-node prisma/seed.ts",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "3.716.0",
    "@aws-sdk/client-s3": "3.726.1",
    "@aws-sdk/client-ses": "3.726.1",
    "@aws-sdk/client-sns": "3.726.1",
    "@nestjs/common": "11.0.14",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "11.0.14",
    "@nestjs/jwt": "11.0.0",
    "@nestjs/passport": "11.0.5",
    "@nestjs/platform-express": "11.0.14",
    "@nestjs/swagger": "11.1.1",
    "@prisma/client": "6.6.0",
    "@types/multer": "1.4.12",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "cookie-parser": "1.4.7",
    "cors": "2.8.5",
    "chalk": "4.1.2",
    "dayjs": "1.11.13",
    "dotenv": "16.4.7",
    "ejs": "3.1.10",
    "email-templates": "12.0.2",
    "jp-zipcode-lookup": "0.3.4",
    "libphonenumber-js": "1.11.17",
    "lodash": "4.17.21",
    "multer": "1.4.5-lts.2",
    "multer-s3": "3.0.1",
    "nodemailer": "6.10.0",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "prisma-class-generator": "0.2.11",
    "randomstring": "1.3.1",
    "reflect-metadata": "0.2.2",
    "rimraf": "6.0.1",
    "rxjs": "7.8.2",
    "ts-pattern": "5.7.0",
    "winston": "3.17.0",
    "winston-daily-rotate-file": "4.7.1",
    "winston-transport": "4.4.2",
    "winston-cloudwatch": "6.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@nestjs/cli": "11.0.6",
    "@nestjs/schematics": "11.0.4",
    "@nestjs/testing": "11.0.14",
    "@swc/cli": "0.5.2",
    "@swc/core": "1.10.7",
    "@types/bcrypt": "5.0.2",
    "@types/express": "5.0.1",
    "@types/jest": "29.5.14",
    "@types/lodash": "4.17.16",
    "@types/multer": "1.4.12",
    "@types/node": "22.14.0",
    "@types/passport": "1.0.17",
    "@types/passport-jwt": "4.0.1",
    "@types/passport-local": "1.0.38",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "8.19.1",
    "@typescript-eslint/parser": "8.19.1",
    "eslint": "9.24.0",
    "eslint-config-prettier": "10.1.2",
    "eslint-import-resolver-typescript": "4.3.2",
    "eslint-plugin-import": "2.31.0",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "prisma": "6.6.0",
    "supertest": "7.1.0",
    "ts-jest": "29.3.1",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
